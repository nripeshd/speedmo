        {% if current_tags contains tagHandle %}
      
      
      <div class="filtercheckboxblock"><input type="checkbox" class="css-checkbox" value="{{ tag | handle }}" id="{{ tag | handle }}" checked ><label class="css-label" for="{{ tag | handle }}"> {{ tag }}</label></div>
