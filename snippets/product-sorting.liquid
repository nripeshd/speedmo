<script>
  $(document).ready(function(){
       
     $('#collectionFilters .selectcontainer ul, .brandwise-collection .brandwise-list ul').hide();
         
  /*  $( ".selectcontainer, .brandwise-list" ).mouseenter(function() {
        $(this).find('ul').css('display','block');
      }).mouseleave(function() {
        $(this).find('ul').css('display','none');
      });   */
    
    if(jQuery(window).width() > 1024){  
      $( ".selectcontainer, .brandwise-list" ).mouseenter(function() {
        $(this).find('ul').css('display','block');
      }).mouseleave(function() {
        $(this).find('ul').css('display','none');
      });    
    }else{    
      $('.selectcontainer h3').click(function(){ 
        $('.selectcontainer h3').not(this).parent().find('ul').hide();
        $('.selectcontainer h3').not(this).parent().removeClass('active');
         $(this).parent().toggleClass('active'); 
//         if($(this).parent().hasClass('active')){
//            $('.selectcontainer').find('ul').hide();
//            $(this).parent('.active').find('ul').show();
//         }else{
//            $(this).parent().find('ul').hide();
//         }
         $(this).parent().find('ul').toggle();
      });
    }

var allFilters = jQuery('.coll-filter input:checkbox, .coll-picker');
    allFilters.click(function() {          
    var newTags = [];
    jQuery('.coll-filter input:checked').each(function() {
      if (jQuery(this).val()) {       
        newTags.push(jQuery(this).val());
       var get_id_scroll = jQuery(this).val()
       
      }
    });
    if (newTags.length) {
    
      var query = newTags.join('+');
      if (jQuery('.coll-picker').length) {
        window.location.href = '/collections/' + jQuery('.coll-picker').val() + '/' + query;                 
      }else {
        window.location.href = jQuery('{{ 'tag' | link_to_tag: 'tag' }}').attr('href').replace('tag', query);        
      }
    }else {
      if (jQuery('.coll-picker').length) {
        window.location.href = '/collections/' + jQuery('.coll-picker').val();
      }else {
        {% if collection.handle %}
        window.location.href = '/collections/{{ collection.handle }}';
        {% elsif collection.products.first.type == collection.title %}
        window.location.href = '{{ collection.title | url_for_type }}';
        {% elsif collection.products.first.vendor == collection.title %}
        window.location.href = '{{ collection.title | url_for_vendor }}';
        {% endif %}
      }
    }
  });
    
    var currentURL = document.URL;
     var splitedURL_first = currentURL.split("?")[0];
    
    var splitedURL = splitedURL_first.split("/");
    
    if(splitedURL.length > 5)
    {

      var currentTags = splitedURL[5].split("+");
      for(var i=0;i< currentTags.length;i++)
      {
        $('#materialsFiltercheck #'+currentTags[i]).prop('checked', true);
        $('#shapeFiltercheck #'+currentTags[i]).prop('checked', true);
        $('#patternFiltercheck #'+currentTags[i]).prop('checked', true);
        $('#styleFiltercheck #'+currentTags[i]).prop('checked', true);
        $('#backheight #'+currentTags[i]).prop('checked', true);
        $('#vendorsFiltercheck #'+currentTags[i]).prop('checked', true);
      }
    }
    if($('#colorFilter input').length == 0){
    	 $(".colorf").hide();  
    } 
    if($('#materialsFiltercheck input').length == 0){
    	 $(".materialsf").hide();  
    }
    if($('#styleFiltercheck input').length == 0){
   	  	$(".stylef").hide();  
    }
    if($('#backheight input').length == 0){
       $(".backheight").hide();  
    }
    if($('#patternFiltercheck input').length == 0){
    	 $(".patternf").hide();  
    }
    if($('#shapeFiltercheck input').length == 0){
    	 $(".shapef").hide();  
    }
    if($('#vendorsFiltercheck input').length == 0){
    	 $(".vendorsf").hide();  
    }  
    if($('#colorFilter input').length == 0  && $('#materialsFiltercheck input').length == 0 && $('#patternFiltercheck input').length ==  0 && $('#styleFiltercheck input').length == 0 && $('#shapeFiltercheck input').length == 0 && $('#backheight input').length == 0 && $('#vendorsFiltercheck input').length == 0){      	
      	$('.filterbytitle').hide();
    }
      
      if(window.pageYOffset == 0 && $('.collectionproducttitle').length > 0){      
        if(splitedURL.length > 5){ setTimeout(function(){ $('html,body').animate({ scrollTop: $('.collectionproducttitle').offset().top - 60 },'slow'); },2500);}
      }

});
 $(document).ready(function(){
  $(".filter_search").on("keyup", function() {
    var value = $(this).val().toLowerCase();
   // alert(value);
     $('.filtercheckboxblock .css-label').filter(function() {
        $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
    });
    });
  });
</script>  
